def tinh_S(n):
 
    tu_so = 0
    for i in range(1, n + 1):
        tu_so += i
        
  
    mau_so = 0
    for i in range(2, n + 1, 2):
        mau_so += i
        
    
    if mau_so == 0:
        return 0
        
    return tu_so / mau_so


n = int(input("Nhập số nguyên dương n: "))
ket_qua = tinh_S(n) 
print(f"Kết quả S = {ket_qua}")
