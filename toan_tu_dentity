#toantuidentity.py
a= float(input("Nhập số a: "))
b= float(input("Nhập số b: "))
c=a
print("a is b:", a is b)
print("a is c:", a is c)    
print("a is not b:", a is not b)
print("a is not c:", a is not c)
print("a==b:", a==b)



#sử dụng intering trong python đối với số nhỏ )
x=300
y=300
print("x is y:", x is y) #false vì ngoài khoảng [-5;256]
